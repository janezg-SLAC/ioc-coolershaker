  {ALARM_HIGH_TEMP_RBV, 4, "Alarm - High Temp Warning"}
}

file readHoldingRegister.template {
  pattern {RECORD_NAME, OFFSET, DESC}
  {TEMP_SET_RBV, 0, "temperature read back"}
  {MOTOR_SPEED_SET_RBV, 2, "motor speed read back read back"}
  {MOTOR_DEGREES_SET_RBV, 4, "motor degrees of rotation read back"}
  {MOTOR_DWELL_SET_RBV, 6, "motor dwell time read back"}
}

