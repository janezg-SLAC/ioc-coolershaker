  pattern {RECORD_NAME, OFFSET, DESC}
  {TEMP_SET, 0, "set temperature"}
  {MOTOR_SPEED_SET, 2, "set motor speed"}
  {MOTOR_DEGREES_SET, 4, "set motor degrees of rotation"}
  {MOTOR_DWELL_SET, 6, "set motor dwell time"}
}

